{
    "manifest_version": 2,

    "name": "HMRC A11y Plugin",
    "description": "Check service for a11y v0.2",
    "version": "0.2",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "storage"
    ],
    "web_accessible_resources": [
        "popup.html",
        "/img/*"
    ]
}